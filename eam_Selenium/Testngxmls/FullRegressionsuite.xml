        <package name="com.kp.apps.web.nuvolo.customerPortalTests.*"/>
        <package name="com.kp.apps.web.nuvolo.fam.assetManagement.WorkOrderTests.*"/>
        <package name="com.kp.apps.web.nuvolo.ft.assetManagement.ContractsTests.*"/>
